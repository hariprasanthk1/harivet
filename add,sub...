#include <stdio.h>

int main() {
    int a, b, sum,sub, product;
    
    printf("Enter the first number: ");
    scanf("%d", &a);
    printf("Enter the second number: ");
    scanf("%d", &b);

    sum = a + b;
    sub = a - b;
    product = a * b;
    
    printf("Sum: %d\n", sum);
    printf("sub: %d\n",sub);
    printf("Product: %d\n", product);

    return 0;
}
